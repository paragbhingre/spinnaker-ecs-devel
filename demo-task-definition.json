{
  "family": "PLACEHOLDER",
  "containerDefinitions": [
    {
      "name": "main",
      "image": "PLACEHOLDER",
      "portMappings": [
        {
          "hostPort": 80, 
          "protocol": "tcp",
          "containerPort": 80
        }
      ],
      "environment": [
        {
          "name": "PORT",
          "value": "80"
        }
      ],
      "cpu": 256,
      "memoryReservation": 512,
      "essential": true
    }
  ],
  "cpu": "256",
  "memory": "512",
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "executionRoleArn": "SpinnakerManaged",
  "networkMode": "awsvpc"
}
